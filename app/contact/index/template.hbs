<Layout 
    @aside={{help}}>
  <McWrapper
    @heading="Contacts"
    @role="All contacts">
      <div class="inline">
        <FormFields::Submit
          @classess="ui large default fluid button ellipse"
          @title={{title}}
          @perform={{action "perform" }}
        />
      </div>
    {{!-- {{#each model as |contact| }} --}}
      <div class="mc-divider m-b-15"></div>
      <ContactLabel
        @contact={{contact}}
        @name={{user.fullname}}
        @email={{user.email}}
        @emailAlt="dashie@dropbox.com.macosa"
        @userImage="https://pbs.twimg.com/media/C1BQ_RmVEAE3ng4.jpg"
      />
    {{!-- {{/each}} --}}
  </McWrapper>
</Layout>